  "description": "Chainable, immutable, dependecy-free library to make text search and replace more readable",
  "main": "index.js",
  "scripts": {
    "build": "tsc && uglifyjs -c -m dist/Look.js -o Look.min.js",
    "test": "ts-mocha **/*.test.ts",
    "test:watch": "ts-mocha **/*.test.ts -w --watch-files **/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robertrosman/look-in.git"
  },
  "keywords": [
