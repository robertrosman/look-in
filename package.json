  "license": "MIT",
  "bugs": {
    "url": "https://github.com/robertrosman/look-in/issues"
  },
  "homepage": "https://github.com/robertrosman/look-in#readme",
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "chai": "^4.3.6",
    "mocha": "^10.0.0",
    "ts-mocha": "^10.0.0",
    "typescript": "^4.6.4"
  }
}
